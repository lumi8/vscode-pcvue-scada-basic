{
  "name": "pcvue-scada-basic",
  "displayName": "PcVue SCADA Basic",
  "description": "For PcVue SCADA Basic programming language",
  "repository": {
    "type": "git",
    "url": "https://github.com/lumi8/vscode-pcvue-scada-basic.git"
  },
  "version": "0.0.5",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "scada-basic",
      "aliases": ["PcVue SCADA Basic", "scada-basic"],
      "extensions": [".scb",".svb",".prg"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "scada-basic",
      "scopeName": "source.scadabasic",
      "path": "./syntaxes/scada-basic.tmLanguage.json"
    }],
    "snippets": [
        {
            "language": "scada-basic",
            "path": "./snippets/scada-basic-snippets.json"
        }
    ]
  }
}
